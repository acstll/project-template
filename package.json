{
  "name": "project-www",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "css": "postcss -u postcss-import -u postcss-cssnext -o OUTPUT ./styles/main.css",
    "css:watch": "watch 'npm run css' ./styles",
    "css:nano": "postcss -u postcss-import -u postcss-cssnext -u cssnano -o OUTPUT ./styles/main.css",
    "bf": "browserify -t babelify ./scripts/main.js > OUTPUT",
    "wf": "watchify ./scripts/main.js -t babelify -v -o OUTPUT",
    "uglify": "uglifyjs INPUT -mc --preamble '/*! :Â·) */' -o OUTPUT",
    "disc": "browserify -t babelify ./scripts/main.js --full-paths | discify --open",
    "lint": "standard scripts/*"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2040": "^1.1.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "cssnano": "^3.7.6",
    "disc": "^1.3.2",
    "postcss": "^5.2.2",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "standard": "^8.2.0",
    "uglifyjs": "^2.4.10",
    "watch": "^1.0.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "normalize.css": "^4.2.0"
  }
}
