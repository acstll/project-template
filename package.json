{
  "name": "project-www",
  "version": "1.0.0",
  "private": true,
  "config": {
    "preamble": "/*! arturu.com */"
  },
  "scripts": {
    "myth": "myth ./styles/main.css > OUTPUT",
    "myth:watch": "dirwatch -c 'npm run myth' ./styles",
    "bf": "browserify ./scripts/main.js > OUTPUT",
    "wf": "watchify ./scripts/main.js -o OUTPUT",
    "uglify": "uglifyjs INPUT -mc --preamble $npm_package_config_preamble -o OUTPUT",
    "disc": "browserify ./scripts/main.js --full-paths | discify --open"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "dirwatch": "^1.1.0",
    "disc": "^1.3.2",
    "myth": "^1.4.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^2.4.0"
  }
}
